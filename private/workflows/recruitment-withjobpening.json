  "schemas": {
    "CONTRACT": {
      "type": "object",
      "title": "Client Contract",
      "properties": {
        "job_title": {
          "title": "Job Title",
          "type": "string"
        },
        "description": {
          "title": "Description",
          "type": "string"
        },
        "jobstatus": {
          "title": "Job Opening Status",
          "type": "string",
          "enum": [
            "Open"
          ]
        },
        "customer_name": {
          "title": "Customer Name",
          "type": "string"
        },
        "customer_type": {
          "title": "Customer Type",
          "type": "string"
        },
        "customer_group": {
          "title": "Customer Group",
          "type": "string"
        },
        "territory": {
          "title": "Geographical Territory",
          "type": "string"
        }
      },
      "required": []
    },
    "APPLICANTS": {
      "type": "object",
      "title": "Job Applicant",
      "properties": {
        "job_title": {
          "title": "Job Title",
          "type": "string"
        },
        "job_name": {
          "title": "Job name",
          "type": "string"
        },
        "description": {
          "title": "Description",
          "type": "string"
        },
        "jobstatus": {
          "title": "Job Opening Status",
          "type": "string"
        },
        "customer_name": {
          "title": "Customer Name",
          "type": "string"
        },
        "customer_type": {
          "title": "Customer Type",
          "type": "string"
        },
        "customer_group": {
          "title": "Customer Group",
          "type": "string"
        },
        "territory": {
          "title": "Geographical Territory",
          "type": "string"
        },
        "applicant_name": {
          "title": "Applicant Name",
          "type": "string"
        },
        "applicantstatus": {
          "title": "Applicant Status",
          "type": "string",
          "enum": [
            "Open",
            "Replied",
            "Rejected",
            "Hold"
          ]
        },
        "email_id": {
          "title": "Email Address",
          "type": "string",
          "pattern": "^[^@^-]+@[^@^.]+\\.[a-zA-Z0-9]{2,}(\\.[a-zA-Z0-9]{2,})?$",
          "validationMessage": "Please enter in a proper email format"
        },
        "cover_letter": {
          "title": "Cover letter",
          "type": "string"
        },
        "SalaryID": {
          "title": "Fixed Monthly Salary SGD",
          "type": "string"
        },
        "JobSearchID": {
          "title": "Occuptation",
          "type": "string"
        },
        "ExperienceID": {
          "title": "Total years of working experience",
          "type": "string"
        },
        "ExperienceLocalID": {
          "title": "Years of working experience in Singapore",
          "type": "string"
        },
        "numberOfEducation": {
          "title": "Number of educational Qualifications",
          "type": "string"
        },
        "countrySelected": {
          "title": "Country of qualification",
          "type": "string"
        },
        "stateSelected": {
          "title": "State",
          "type": "string"
        },
        "nameSelected": {
          "title": "Name",
          "type": "string"
        },
        "firstQual": {
          "title": "Qualifications",
          "type": "string"
        },
        "firstFacultySel": {
          "title": "Faculty",
          "type": "string"
        },
        "firstEduSpecSelected": {
          "title": "Specialisation",
          "type": "string"
        },
        "firstStudymodeID": {
          "title": "Mode of Study",
