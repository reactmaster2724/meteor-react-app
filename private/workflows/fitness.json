          },
          {
            "title": "Blood Group",
            "key": "BloodGroup",
            "htmlClass": "half-width label-left",
            "type": "label"
          },
          {
            "title": "Past Injury Records",
            "key": "PastInjuryRecords",
            "htmlClass": "half-width label-left",
            "type": "label"
          },
          {
            "title": "Allergy",
            "key": "Allergy",
            "htmlClass": "half-width label-left",
            "type": "label"
          },
          {
            "key": "Emergencycontactname",
            "htmlClass": "half-width label-left",
            "title": "Emergency Contact Name",
            "type": "label"
          },
          {
            "key": "Emergencycontactrelationship",
            "htmlClass": "half-width label-left",
            "title": "Emergency Contact Relationship",
            "type": "label"
          },
          {
            "title": "Emergency Contact Number",
            "key": "EmergencyContact",
            "htmlClass": "half-width label-left",
            "type": "label"
          },
          {
            "title": "Height (cm)",
            "key": "Height",
            "htmlClass": "half-width label-left",
            "type": "label"
          },
          {
            "key": "Weight",
            "title": "Weight",
            "htmlClass": "half-width label-left"
          },
          {
            "key": "FatMassPercentage",
            "title": "Fat Mass %",
            "htmlClass": "half-width label-left"
          },
          {
            "key": "MuscleMassPercentage",
            "title": "Muscle Mass %",
            "htmlClass": "half-width label-left"
          },
          {
            "key": "BMI",
            "title": "BMI",
            "htmlClass": "half-width label-left",
            "type": "total",
            "expression": "round(Weight / ((Height/100)^2), 1)"
          },
          {
            "key": "Classification",
            "title": "Classification",
            "htmlClass": "half-width label-left",
            "type": "ifttt",
            "disable": "true",
            "expression": {
              "if": {
                "condition": "BMI < 19",
                "then": {
                  "if": {
                    "condition": "not BMI or BMI == NaN or BMI == Infinity",
                    "then": {
                      "type": "select",
                      "option": "No input"
                    }
                  },
                  "else": {
                    "then": {
                      "type": "select",
                      "option": "Underweight"
                    }
                  }
                }
              },
              "else": {
                "then": {
                  "if": {
                    "condition": "BMI < 25",
                    "then": {
                      "type": "select",
                      "option": "Normal Weight"
                    }
                  },
                  "else": {
                    "then": {
                      "if": {
                        "condition": "BMI < 30",
                        "then": {
                          "type": "select",
                          "option": "Overweight"
                        }
                      },
                      "else": {
                        "then": {
                          "if": {
                            "condition": "BMI < 35",
                            "then": {
                              "type": "select",
                              "option": "Obesity level 1"
                            }
                          },
                          "else": {
                            "then": {
                              "if": {
                                "condition": "BMI < 40",
                                "then": {
                                  "type": "select",
                                  "option": "Obesity level 2"
                                }
                              },
                              "else": {
                                "then": {
                                  "type": "select",
                                  "option": "Obesity level 3"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          {
            "type": "fieldset",
            "title": "Health Check",
            "notitle": true,
            "items": [
              {
                "key": "Systolic",
                "htmlClass": "half-width label-left",
                "title": "Systolic (mmHg)"
              },
              {
                "key": "Diastolic",
                "htmlClass": "half-width label-left",
                "title": "Diastolic (mmHg)"
              },
              {
                "key": "RestHeartRate",
                "htmlClass": "half-width label-left",
                "title": "Resting Heart Rate"
              },
              {
                "key": "BloodPressureClassification",
                "htmlClass": "half-width label-left",
                "title": "Blood Pressure Classification",
                "type": "ifttt",
                "disable": true,
                "expression": {
                  "if": {
                    "condition": "not Systolic or not Diastolic",
                    "then": {
                      "type": "select",
                      "option": "No input"
                    }
                  },
                  "else": {
                    "then": {
                      "if": {
                        "condition": "Systolic < 120 and Diastolic < 80",
                        "then": {
                          "type": "select",
                          "option": "Optimal blood pressure"
                        }
                      },
                      "else": {
                        "then": {
                          "if": {
                            "condition": "Systolic < 130 and Diastolic < 85",
                            "then": {
                              "type": "select",
                              "option": "Normal blood pressure"
                            }
                          },
                          "else": {
                            "then": {
                              "if": {
                                "condition": "Systolic < 140 and Diastolic < 90",
                                "then": {
                                  "type": "select",
                                  "option": "Normal systolic value"
                                }
                              },
                              "else": {
                                "then": {
                                  "if": {
                                    "condition": "Systolic < 160 and Diastolic < 100",
                                    "then": {
                                      "type": "select",
                                      "option": "Mild hypertension"
                                    }
                                  },
                                  "else": {
                                    "then": {
                                      "if": {
                                        "condition": "Systolic < 180 and Diastolic < 110",
                                        "then": {
                                          "type": "select",
                                          "option": "Moderate hypertension"
                                        }
                                      },
                                      "else": {
                                        "then": {
                                          "type": "select",
                                          "option": "Severe hypertension"
                                        }
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "key": "watchData",
        "title": "Watch Data",
        "type": "table",
